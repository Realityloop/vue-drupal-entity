{
  "name": "vue-drupal-entity",
  "version": "0.1.0",
  "description": "Render Drupal Entities and fields with Drupal JSON:API Entities schema support.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Realityloop/vue-drupal-entity"
  },
  "keywords": [
    "drupal",
    "druxt",
    "entity",
    "fields",
    "nuxt",
    "json-api",
    "jsonapi",
    "json api"
  ],
  "author": "Stuart Clark <stuart@realityloop.com> (realityloop.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Realityloop/vue-drupal-entity/issues"
  },
  "homepage": "https://github.com/Realityloop/vue-drupal-entity#readme",
  "main": "dist/vue-drupal-entity.ssr.js",
  "module": "dist/vue-drupal-entity.esm.js",
  "unpkg": "dist/vue-drupal-entity.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/app.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-buble": "^0.20.0",
    "@rollup/plugin-replace": "^2.2.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "minimist": "^1.2.0",
    "rollup": "^1.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-vue": "6.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  }
}
